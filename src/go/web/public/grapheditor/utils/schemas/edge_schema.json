{
  "$id": "#/vlans",
  "type": "object",
  "title": "VLAN",
  "required": [
    "id",
    "name"
  ],
  "properties": {
    "id": {
      "$id": "#/vlans/properties/id",
      "type": "integer",
      "title": "ID",
      "default": 0,
      "examples": [
        101
      ]
    },
    "name": {
      "$id": "#/vlans/properties/name",
      "type": "string",
      "title": "Name",
      "minLength": 1,
      "default": "",
      "examples": [
        "SCADAPWR"
      ],
      "pattern": "^(.*)$"
    }
  }
}